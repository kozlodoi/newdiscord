{
  "name": "discord-simple",
  "version": "1.0.0",
  "scripts": {
    "start": "node server.js",
    "dev": "node --watch server.js",
    "build": "npm install && npx prisma generate && npx prisma db push --accept-data-loss",
    "db:generate": "npx prisma generate",
    "db:push": "npx prisma db push --accept-data-loss",
    "db:reset": "npx prisma db push --force-reset",
    "db:studio": "npx prisma studio",
    "init": "npm install && npx prisma generate && npx prisma db push --accept-data-loss"
  },
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.4",
    "@prisma/client": "^5.10.2",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "prisma": "^5.10.2"
  }
}
